{
  "metadata": [
    {
      "src": [
        {
          "files": ["**/*.csproj"],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "./src"
        }
      ],
      "dest": "build/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ],
        "dest": "./api",
        "src": "build"
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "articles/toc.yml",
          "articles/*.md"
        ],
        "dest": "./articles",
        "src": "./docfx"
      },
      {
        "files": [
          "articles/index.md"
        ],
        "dest": ".",
        "src": "."
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "./docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "templates/discordfx"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}